package pruebasFicheros;
import java.io.*;
public class CopiaPrueba {
	
	public static void main(String[]args){
		
	
		String rutaIn=".\\src\\pruebasFicheros\\CopiaPruebas.txt";
		String rutaOut=".\\src\\pruebasFicheros\\FicheroCopiado.txt";
		FileWriter fw=null;
		FileReader fr=null;
		try
		{
			fw=new FileWriter(rutaIn);
			fr=new FileReader(rutaOut);
		}catch(IOException e)
		{
			System.out.println("Error en E/S: "+e);
		}
	}
}
