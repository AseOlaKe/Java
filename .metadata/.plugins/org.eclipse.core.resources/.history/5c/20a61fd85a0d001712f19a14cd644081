package ordenacionArrays;

public class Avanzados {
	
	//i se inicializa a 1(2da posicion del array)
	public static void metodoInsercion(int[]array)
	{
		int i, j, aux;
		for(i=1;i<array.length;i++)
		{
			for(j=i;j>0&&array[j-1]>array[j];j--)
			{
				aux = array[j]; 
				array[j] = array[j-1];
				array[j-1] = aux;
			}
		}
	}
	
	

}