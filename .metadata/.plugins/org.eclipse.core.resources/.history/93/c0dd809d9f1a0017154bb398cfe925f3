package pruebasFicheros;
import java.io.*;
public class CopiaPrueba {
	
	public static void main(String[]args){
		
	
		String ruta=".\\src\\pruebasFicheros\\CopiaPruebas.txt";
		FileWriter fw=null;
		try
		{
			fw=new FileWriter(ruta);
		}catch(IOException e)
		{
			System.out.println("Error en E/S: "+e);
		}
	}
}
